{
  "hosting": [
    {
      "public": "dist/alexander",
      "ignore": [
        "firebase.json",
        "/.*",
        "/node_modules/**"
      ],
      "target": "alexander",
      "rewrites": [
        {
          "source": "/",
          "destination": "/fr/index.html"
        },
        {
          "source": "/fr/**",
          "destination": "/fr/index.html"
        },
        {
          "source": "/en/**",
          "destination": "/en/index.html"
        },
        {
          "source": "/ro/**",
          "destination": "/ro/index.html"
        }
      ],
      "i18n": {
        "root": "/"
      },
      "frameworksBackend": {
        "region": "us-central1"
      },
      "redirects": [ {
        "source": "/",
        "destination": "https://sitedemo.alexandertudose.com/",
        "type": 301
      } ]    
    }
  ]
}